{
	"name": "prompt-kit",
	"description": "prompt-kit: developer prompt utilities",
	"version": "0.0.0",
	"author": "YogliB",
	"bugs": "https://github.com/YogliB/prompt-kit/issues",
	"devDependencies": {
		"@eslint/compat": "1.4.0",
		"eslint": "9.37.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.4",
		"husky": "9.1.7",
		"lint-staged": "16.2.2",
		"prettier": "3.6.2"
	},
	"homepage": "https://github.com/YogliB/prompt-kit",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"prepare": "volta setup && husky",
		"format": "prettier --write .",
		"lint": "eslint --ext .js,.ts .",
		"lint:fix": "eslint --ext .js,.ts . --fix"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix"
		],
		"!*.{js,ts}": [
			"prettier --write --ignore-unknown"
		]
	},
	"volta": {
		"node": "22.20.0"
	}
}
